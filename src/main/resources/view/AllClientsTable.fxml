<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="clientTablePane" prefHeight="427.0" prefWidth="649.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.assignment.project.controller.AllClientsTableController">
    <children>
        <TableView fx:id="tblClients" prefHeight="400.0" prefWidth="651.0">
            <columns>
                <TableColumn fx:id="colClientId" prefWidth="100.0" text="Client ID" />
                <TableColumn fx:id="colClientName" prefWidth="150.0" text="Name" />
                <TableColumn fx:id="colClientAddress" prefWidth="150.0" text="Address" />
                <TableColumn fx:id="colClientPhone" prefWidth="100.0" text="Phone" />
                <TableColumn fx:id="colClientEmail" prefWidth="150.0" text="Email" />
            </columns>
        </TableView>
      <JFXButton fx:id="btnCloseTable" layoutX="577.0" layoutY="400.0" onAction="#onClientTableClose" prefHeight="27.0" prefWidth="74.0" style="-fx-background-color: #ff3f34;" text="Close" textFill="WHITE">
         <font>
            <Font name="System Bold" size="13.0" />
         </font></JFXButton>
    </children>
</AnchorPane>
